{
  "name": "coloursfactory/refreshmailer",
  "description": "Implementation of FreshMail.pl REST API",
  "keywords": [
    "freshmail",
    "api",
    "rest",
    "curl",
    "guzzle"
  ],
  "type": "package",
  "license": "MIT",
  "authors": [
    {
      "name": "Konrad Pawlikowski",
      "email": "preclowski@gmail.com",
      "homepage": "https://github.com/preclowski"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "ext-curl": "*",
    "ext-json": "*",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6",
    "guzzlehttp/guzzle": "^6"
  },
  "suggest": {
    "guzzlehttp/guzzle": "^6"
  },
  "autoload": {
    "psr-4": {
      "ColoursFactory\\ReFreshMailer\\": "src/"
    }
