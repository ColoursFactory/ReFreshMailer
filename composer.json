{
  "name": "coloursfactory/refreshmailer",
  "description": "Implementation of FreshMail.pl REST API",
  "keywords": [
    "freshmail",
    "curl",
    "guzzle",
    "php-http",
    "httplug"
  ],
  "type": "package",
  "license": "MIT",
  "authors": [
    {
      "name": "Konrad Pawlikowski",
      "email": "preclowski@gmail.com",
      "homepage": "https://github.com/preclowski"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "ext-curl": "*",
    "ext-json": "*",
    "psr/http-message": "^1.0",
    "php-http/client-common": "^1.5",
    "php-http/client-implementation": "^1.0",
    "php-http/discovery": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^6",
    "php-http/mock-client": "^1.0",
    "php-http/curl-client": "^1.7",
    "php-http/guzzle6-adapter": "^1.0",
    "php-http/message": "^1.5",
    "guzzlehttp/guzzle": "^6||^5",
    "guzzlehttp/psr7": "^1.4"
  },
  "suggest": {
    "guzzlehttp/guzzle": "^6||^5",
    "guzzlehttp/psr7": "^1.4",
    "php-http/curl-client": "^1.7",
    "php-http/mock-client": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "ColoursFactory\\ReFreshMailer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ColoursFactory\\ReFreshMailer\\Tests\\": "tests/"
    }
  }
}
